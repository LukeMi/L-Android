<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EmotionalFaceView">
        <attr name="borderWidth" format="dimension" />
        <attr name="borderColor" format="reference|color|integer" />
        <attr name="faceColor" format="reference|color|integer" />
        <attr name="eyesColor" format="reference|color|integer" />
        <attr name="mouthColor" format="reference|color|integer" />
        <attr name="emotionStyle" format="enum">
            <enum name="happy" value="1" />
            <enum name="sad" value="0" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RoundAngleImageView">
        <attr name="roundWidth" format="dimension" />
        <attr name="roundHeight" format="dimension" />
    </declare-styleable>

    <!--流式布局-->
    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer"></attr>
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

</resources>